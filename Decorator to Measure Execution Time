
import time

def timer(func):
    def wrapper(*args, **kwargs):
        start = time.time()
        result = func(*args, **kwargs)
        print(f"{func.__name__} executed in {time.time()-start:.4f}s")
        return result
    return wrapper

@timer
def compute():
    return sum(i*i for i in range(1000000))

compute()
